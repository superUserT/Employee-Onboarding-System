@model technicalAssesment.Models.Domain.Customer
@addTagHelper*, Microsoft.AspNetCore.Mvc.TagHelpers
@using System.Data



@{
    ViewData["Title"] = "Read More";
}

<h1>Customer Details</h1>

<div>
    <h4>Customer</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.FirstName)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.LastName)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.cellphone)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.comment)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.physicalAddressLine)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.physicalSuburb)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.physicalCountry)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.physicalPostalCode)
        </dd>
    </dl>
</div>
<div>
   
    <a asp-controller="Customer" asp-action="Index">Back to List</a>
</div>
